//登录后首页
if($(".WWelcome").length > 0){
$(".Footer,.BodyBg").hide();
	$(window.parent.document).find(".Footer,.BodyBg").hide();
    $(window.parent.document).find(".Footer,.BodyBg").hide();
    $(window.parent.document).find("#mainFrame").height("1055");
    $(window.parent.document).find("#mainFrame").css("margin","0");
}else{
    $(window.parent.document).find(".Footer,.BodyBg").show();
    $(window.parent.document).find("#mainFrame").removeAttr("style");
	if($(window.parent.document).find("#mmcIf").length > 0){
		setTimeout(function(){
			$(".Footer,.BodyBg").show();
			$("#mainFrame").removeAttr("style");
		},100)
	}
}