<script type="text/html" id="content_115">
    {{each list value i}}
    <tr>
        <td class="issue">
            <!-- 输出期号 -->
            <div>{{value.issue}}</div>
        </td>
        <!-- 输出开奖号 -->
        {{each value.code codeItem}}
        <td align="center">
            <div class="ballnumb">
                <div class="wth">{{codeItem}}</div>
            </div>
        </td>
        {{/each}}
        <!--循环个十百千万位数-->
        <% for(var j=0; j< 5; ++j) { %>
        <!--循环1-11的数字-->
        <% for (var i=1; i<= 11; ++i) { %>
        <!--判断是第几列，是奇数列还是偶数列-->
        {{set rem = j%2}}
        <!--判断开奖号是否在1-11的数字上，如果是则输出带样式的球号-->
        <% if(value.code[j] == i) { %>
        <td align="center" class="charball">
            <div class="tenthousand">
                <!-- 根据单双数列判断是红色还是蓝色 -->
                <div class="ball0{{rem>0?2:1}}">{{value.code[j]}}</div>
            </div>
        </td>
        <% } else { %>
        <td align="center" class="wdh">
            <div class="tenthousand">
                <div class="ball0{{rem>0?4:3}}"></div>
            </div>
        </td>
        <% } %>
        <% } %>
        <% } %>
    </tr>
    {{/each}}
</script>
