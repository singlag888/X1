        <td align="center">
            <div class="ballnumb">
                <div class="wth">{{codeItem}}</div>
            </div>
        </td>
        {{/each}}

        <!--循环冠军亚军三四五六名-->
        <% for(var j=0; j< 10; ++j) { %>
        <!--循环每一名上的10个号码-->
        <% for(var i = 1; i< 11; ++i ) { %>
        <!--判断第几列，是单数列还是双数列-->
        {{set rem =j%2 }}
        <!--判断是否为开奖号码，如果是则输出的带有样式的开奖号-->
        <% if(value.code[j] == i) { %>
        <td align="center" class="charball">
            <div class="tenthousand">
                <div class="ball0{{rem>0?2:1}}">{{value.code[j]}}</div>
            </div>
        </td>
        <% } else{ %>
        <td align="center" class="wdh">
            <div class="tenthousand">
                <div class="ball0{{rem>0?4:3}}"></div>
            </div>
        </td>
        <% } %>
        <% } %>
        <% } %>
    </tr>
    {{/each}}
</script>
