<script type="text/html" id="content_ssc">
    {{each list value i}}
    <tr>
        <td class="issue">
            <!-- 输出期号 -->
            <div>{{value.issue}}</div>
        </td>

        <!-- 输出开奖号 -->
        {{each value.code codeItem}}
        <td align="center">
            <div class="ballnumb">
                <div class="wth">{{codeItem}}</div>
            </div>
        </td>
        {{/each}}
        <!-- 循环万千百十个位 -->
        <% for(var j = 0; j < value.code.length; ++j){ %>
        <!-- 循环每一位上的10个号 -->
        <% for(var i = 0; i < 10; ++i){ %>
        <!-- 判断第几列,单数列还是双数列 -->
        {{set rem = j%2}}
        <!-- 判断是否是开奖的号,是则输出圈的样式 -->
        <% if(value.code[j] == i){ %>
        <td align="center" class="charball">
            <div class="tenthousand">
                <!-- 根据单双数列判断是红色还是蓝色 -->
                <div class="ball0{{rem>0?2:1}}">{{value.code[j]}}</div>
            </div>
        </td>
        <% }else{ %>
        <td align="center" class="wdh">
            <div class="tenthousand">
                <div class="ball0{{rem>0?4:3}}"></div>
            </div>
        </td>
        <% } %>
        <% } %>
        <% } %>
    </tr>
    {{/each}}
</script>
